  <body>

    <!-- Your banner code -->
    <div class="banner" style="background-image: linear-gradient(-225deg, rgba(0,101,168,0.6) 0%, rgba(0,36,61,0.6) 50%), url('images/background.jpg');">
      <div class="banner-content text-center">
        <h1>WELCOME HOME</h1>
        <p>Rent unique places to stay from local hosts in 190+ countries.</p>
        <a class="btn btn-primary btn-lg">Let's go...</a>
      </div>
    </div>

    <!-- Your features code -->
      <div class="wrapper-white">
      <div class="container">
        <div class="row">
          <div class="col-xs-12 col-sm-6 col-lg-3">
            <div class="features">
              <img src="images/globe.png" alt="">
              <h2>Discover the world</h2>
              <p>The earth is your new home</p>
            </div>
          </div>

          <div class="col-xs-12 col-sm-6 col-lg-3">
            <!-- card goes here -->
            <div class="features">
              <img src="images/dollar.png" alt="">
              <h2>Pay online</h2>
              <p>No cash issues anymore</p>
            </div>
          </div>

          <div class="col-xs-12 col-sm-6 col-lg-3">
            <!-- card goes here -->
            <div class="features">
              <img src="images/beer.png" alt="">
              <h2>Meet people</h2>
              <p>Discover new culture and lifestyle</p>
            </div>
          </div>

          <div class="col-xs-12 col-sm-6 col-lg-3">
            <!-- card goes here -->
            <div class="features">
              <img src="images/heart.png" alt="">
              <h2>Share and love</h2>
              <p>Join a community you will love</p>
            </div>
          </div>
        </div>
      </div>
    </div>

    <!-- Your cards code -->
    <div class="wrapper-grey">
      <div class="container">
        <h2 class="text-center">Your next trip</h2>
        <div class="row">
          <div class="col-xs-12 col-sm-4">
            <div class="card" style="background-image: linear-gradient(-225deg, rgba(30,30,30,0.6) 30%, rgba(46,46,46,0.5) 80%), url('images/terrace.png');">
              <h3 class="card-category">Melbourne</h3>
              <h2 class="card-user">Alicia's Terrace</h2>
              <p class="card-description">Hang out at a classic Victorian terrace.</p>
              <img src="images/profile_pic.png" alt="user" class="card-avatar">
              <a class="card-link" href="#"></a>
            </div>
          </div>

          <div class="col-xs-12 col-sm-4">
            <div class="card" style="background-image: linear-gradient(-225deg, rgba(30,30,30,0.6) 30%, rgba(46,46,46,0.5) 80%), url('images/queenslander.png');">
              <h3 class="card-category">Brisbane</h3>
              <h2 class="card-user">Eli's Queenslander</h2>
              <p class="card-description">Explore the heart of sunny Brisbane.</p>
              <img src="images/eli.png" alt="user" class="card-avatar">
              <a class="card-link" href="#"></a>
            </div>
          </div>

          <div class="col-xs-12 col-sm-4">
            <div class="card" style="background-image: linear-gradient(-225deg, rgba(30,30,30,0.6) 30%, rgba(46,46,46,0.5) 80%), url('images/card-brussels.jpeg');">
              <h3 class="card-category">Brussels</h3>
              <h2 class="card-user">Anne's Home</h2>
              <p class="card-description">Lovely House</p>
              <img src="images/anne.jpg" alt="user" class="card-avatar">
              <a class="card-link" href="#"></a>
            </div>
          </div>

        </div>
      </div>
    </div>

    <!-- Your newsletter code -->
  <div class="form-wrapper wrapper-white">
    <div class="container">
      <div class="row">
        <div class="col-sm-offset-4 col-sm-4">
      <!-- Le contenu de la div centrÃ©e -->
          <h2 class="text-center">Get news from us...</h2>
          <p class="text-center">Enter your email address.</p>

            <form class="form-inline">
              <input type="email" class="form-control" id="exampleInputEmail2" placeholder="jane.doe@example.com">
              <button type="Subscribe" class="btn btn-primary pull-right">Send invitation</button>
            </form>
          </div>
        </div>
      </div>
    </div>

    <!-- Your map code -->

    <div id="map" style="width: 100%; height: 400px;"></div>

    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js"></script>

    <script type="text/javascript">
    var myLatlng = new google.maps.LatLng(48.8655909, 2.378983);

    var myOptions = {
      zoom: 16,
      center: myLatlng,
      scrollwheel: false,
      mapTypeId: google.maps.MapTypeId.ROADMAP,
      styles: [] // TODO: replace [] by array from https://snazzymaps.com/
    };

    var map = new google.maps.Map(document.getElementById('map'), myOptions);
    var marker = new google.maps.Marker({
      position: myLatlng,
      map: map,
      title: "You are here!"
    });
    </script>

